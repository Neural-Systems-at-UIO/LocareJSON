{
    "$id": "https://raw.githubusercontent.com/blixilla/LocareJSON/v1.1.0/schemas/locareAtlasMesh.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "atlasMesh": {
            "type": "object",
            "properties": {
                "parcellationEntityVersion": {
                    "type": "string",
                    "format": "uri"
                }
            },
            "required": ["parcellationEntityVersion"]
        },
        "properties": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            },
            "required": ["name", "description"]
        }
    },
    "required": ["atlasMesh", "properties"]
}
